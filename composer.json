{
  "name": "kengineering/sonar-api-client",
  "description": "Sonar API client",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.9",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "phpstan/phpstan": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Kengineering\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kengineering\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "analyse": "vendor/bin/phpstan analyse"
  }
}
